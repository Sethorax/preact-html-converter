{
  "name": "preact-html-converter",
  "version": "0.3.4",
  "description": "Converts a raw HTML string into Preact Elements or even Preact Components. Ships with integrations for node and browser.",
  "keywords": [
    "preact",
    "html"
  ],
  "repository": "https://github.com/Sethorax/react-html-converter.git",
  "author": "Sethorax <info@sethorax.com>",
  "license": "MIT",
  "dependencies": {
    "parse5": "^3.0.2",
    "preact": "^8.2.7"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^23.0.0-alpha.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "bundlesize": "^0.16.0",
    "eslint": "^4.19.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.2",
    "jsdom": "^11.6.2",
    "microbundle": "^0.4.4",
    "preact-render-to-string": "^3.7.0"
  },
  "peerDependencies": {
    "preact": "*"
  },
  "scripts": {
    "test": "eslint src && jest",
    "prepublish": "npm run build",
    "build": "microbundle src/integrations/*.js -o x.js -f cjs --external all --sourcemap false"
  }
}
